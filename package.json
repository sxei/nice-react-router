{
  "name": "nice-react-router",
  "version": "1.0.7",
  "description": "nice-react-router",
  "files": [
    "esm",
    "es2017",
    "cjs",
    "dist"
  ],
  "main": "es2017/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "exports": {
    ".": {
      "es2017": {
        "types": "./es2017/index.d.ts",
        "default": "./es2017/index.js"
      },
      "default": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      }
    },
    "./*": "./*"
  },
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.less",
    "*.css"
  ],
  "scripts": {
    "dev": "ice-pkg start",
    "build": "ice-pkg build",
    "prepublishOnly": "npm run build",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "lint": "npm run eslint && npm run stylelint"
  },
  "keywords": [
    "ice",
    "react",
    "component"
  ],
  "dependencies": {
    "@swc/helpers": "0.4.14",
    "@ice/jsx-runtime": "^0.2.0"
  },
  "devDependencies": {
    "@applint/spec": "^1.2.3",
    "@ice/pkg": "1.5.5",
    "@ice/pkg-plugin-docusaurus": "^1.0.0",
    "eslint": "^8.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "stylelint": "^15.0.0"
  },
  "peerDependencies": {
    "react": "^16"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": "git@github.com:sxei/nice-react-router.git"
}